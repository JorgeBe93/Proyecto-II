	<queryString>
		<![CDATA[SELECT
     seguimiento_actividad.`codigoSeguimiento` AS seguimiento_actividad_codigoSeguimiento,
     seguimiento_actividad.`actividad` AS seguimiento_actividad_actividad,
     seguimiento_actividad.`lugar` AS seguimiento_actividad_lugar,
     seguimiento_actividad.`fechaHora` AS seguimiento_actividad_fechaHora,
     seguimiento_actividad.`codigoEmpleado` AS seguimiento_actividad_codigoEmpleado,
     empleado.`nombre` AS empleado_nombre,
     empleado.`apellido` AS empleado_apellido,
     empleado.`codigoCargo` AS empleado_codigoCargo,
     empleado.`codigoJefe` AS empleado_codigoJefe,
     cargo.`nombre` AS cargo_nombre,
     empleado.`codigoEmpleado` AS empleado_codigoEmpleado,
     actividad.`nombre` AS actividad_nombre,
     lugar.`nombre` AS lugar_nombre
FROM
     `empleado` empleado INNER JOIN `seguimiento_actividad` seguimiento_actividad ON empleado.`codigoEmpleado` = seguimiento_actividad.`codigoEmpleado`
     INNER JOIN `cargo` cargo ON empleado.`codigoCargo` = cargo.`codigoCargo`
     INNER JOIN `actividad` actividad ON seguimiento_actividad.`actividad` = actividad.`codActividad`
     INNER JOIN `lugar` lugar ON seguimiento_actividad.`lugar` = lugar.`codLugar`
WHERE
     str_to_date(fechaHora,'%d-%m-%Y') >= str_to_date($P{FechaDesde},'%d-%m-%Y')
     and str_to_date(fechaHora,'%d-%m-%Y') <= str_to_date($P{FechaHasta},'%d-%m-%Y')
     and empleado.`codigoEmpleado` = $P{CodigoEmp}]]>
	</queryString>
